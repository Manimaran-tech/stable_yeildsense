{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "buildCommand": "cd whirlpool-dashboard && npm install && npm run build",
    "outputDirectory": "whirlpool-dashboard/dist",
    "installCommand": "echo 'Skip root install'",
    "framework": null,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}